{
    "env":{
        "es6": false,
        "browser": true,
        "commonjs": true
    },
    "parser": "esprima",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false,
            "modules": true,
            "impliedStrict": true,
            "allowImportExportEverywhere": false
        }
    },
    "globals":{
        "mqq": true
    },
    "rules": {
        "semi": ["warn"],
        "strict": ["error"],
        "quotes": ["error", "single"],
        "no-use-before-define": ["error"],
        "no-console": [0]
    },
    "extends": "eslint:recommended",
    "fix": true,
    "quiet": true
}